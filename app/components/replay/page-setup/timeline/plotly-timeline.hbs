{{#unless @timestamps.length}}
  <div class="timeline-no-timestamps-outer">
    <div class='timeline-no-timestamps-inner'>
      No replay timestamps available.
    </div>
  </div>
  <div id="asd" class="plotlyDiv timeline-blur-effect"
    {{did-insert this.didRender}}
  >
  </div>
{{else}}
  <div id="asd" class='plotlyDiv'
    {{on "mouseenter" this.handleMouseEnter}}
    {{on "mouseleave" this.handleMouseLeave}}
    {{did-update this.didRender @timestamps}}
    {{did-insert this.didRender}}
  >
  </div>
{{/unless}}