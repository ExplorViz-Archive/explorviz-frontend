  <table style="width:100%">
  <tr>
    <th>TraceID</th>
    <th>Requests</th> 
    <th>Trace Duration</th>
  </tr>
  {{#each traces as |trace|}}
    <tr {{action "traceSelected" trace.traceID}}>
    <td>{{trace.traceID}}
    </td>
    <td>{{trace.requests}}</td>
    <td>{{trace.duration}}</td>
    </tr>
  {{/each}}
</table>