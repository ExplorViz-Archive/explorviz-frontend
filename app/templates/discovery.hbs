{{#if procezzForDetailView}}
  <Discovery::ProcezzDetails @errorHandling={{action this.errorHandling}} @procezz={{this.procezzForDetailView}} />
{{else if this.agentForDetailView}}
  <Discovery::AgentDetails @errorHandling={{action this.errorHandling}} @agent={{this.agentForDetailView}} />
{{else}}
  <Discovery::NodeOverview @showDetails={{action this.showDetailsComponent}} />
{{/if}}